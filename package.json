{
  "name": "htm_jest_reporter",
  "version": "1.1.0",
  "description": "html reporter for jest test cases ",
  "main": "./src/index",
  "scripts": {
    "build": "npm run clean:dist && webpack --config _build/webpack.config.js",
    "clean:dist": "rimraf ./dist/* ./dist/**/*",
    "clean:lib": "rimraf ./lib/* ./lib/**/*",
    "compile:main": "npm run clean:lib && babel ./src -d lib/",
    "lint": "eslint --fix --ext .js --ext .jsx src/",
    "start": "webpack-dev-server --config _build/webpack.dev.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Shreyas Bande",
  "license": "ISC",
  "files": [
    "lib"
  ],
  "dependencies": {
    "react": "16.8.6",
    "react-countup": "4.2.1",
    "react-dom": "16.8.6"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.5",
    "babel-plugin-dynamic-import-node": "2.3.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "css-loader": "2.1.0",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.8.0",
    "react-icons": "3.7.0",
    "source-map-loader": "0.2.4",
    "webpack": "4.16.1",
    "webpack-cli": "3.3.6",
    "webpack-dev-server": "3.7.2",
    "webpack-merge": "4.2.1"
  }
}
